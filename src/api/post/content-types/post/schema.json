{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "urlSlug": {
      "type": "string",
      "required": true
    },
    "Post": {
      "type": "blocks"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Author": {
      "type": "string",
      "required": true
    },
    "postDate": {
      "type": "date",
      "unique": false,
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "posts"
    },
    "Description": {
      "type": "string",
      "required": true
    }
  }
}
